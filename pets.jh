/**
 * The Employee entity.
 */
entity Owner {
	/**
	* The firstname attribute.
	*/
	firstName String,
	lastName String,
	email String
}

entity Pet {
	name String,
	kind String,
	age Integer
}


// defining multiple OneToMany relationships with comments
relationship OneToMany {
	Owner to Pet{owner}
}

// Set service options to all except few
service all with serviceImpl

